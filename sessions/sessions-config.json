{
  "trigger_phrases": {
    "implementation_mode": [
      "yert",
      "do it",
      "implement it"
    ],
    "discussion_mode": [
      "SILENCE",
      "discuss"
    ],
    "task_creation": [
      "mek:",
      "task:add",
      "task:create"
    ],
    "task_startup": [
      "start^",
      "task:start"
    ],
    "task_completion": [
      "finito",
      "task:complete"
    ],
    "context_compaction": [
      "squish",
      "ctx:compact"
    ]
  },
  "blocked_actions": {
    "implementation_only_tools": [
      "Write",
      "Edit",
      "MultiEdit",
      "NotebookEdit"
    ],
    "bash_read_patterns": [],
    "bash_write_patterns": [],
    "extrasafe": true
  },
  "git_preferences": {
    "add_pattern": "all",
    "default_branch": "main",
    "commit_style": "conventional",
    "auto_merge": true,
    "auto_push": true,
    "has_submodules": false
  },
  "environment": {
    "os": "linux",
    "shell": "bash",
    "developer_name": "ts"
  },
  "features": {
    "branch_enforcement": true,
    "task_detection": true,
    "auto_ultrathink": true,
    "icon_style": "nerd_fonts",
    "context_warnings": {
      "warn_85": true,
      "warn_90": true
    }
  }
}
